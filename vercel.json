{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/price",
      "dest": "main.py"
    },
    {
      "src": "/ws",
      "dest": "main.py"
    }
  ]
}